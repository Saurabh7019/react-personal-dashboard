  "requiresCustomScript": false,
  "supportedHosts": ["SharePointWebPart", "TeamsPersonalApp", "TeamsTab", "SharePointFullPage"],
  "supportsThemeVariants": true,

  "preconfiguredEntries": [{
    "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // Advanced
    "group": { "default": "hack-together-teams" },
    "title": { "default": "personalDashboard" },
    "description": { "default": "This web part provides users a personalized dashboard by selecting and adding organization-wide apps." },
    "officeFabricIconFontName": "ViewDashboard",
    "properties": {
      "description": "This web part provides users a personalized dashboard by selecting and adding organization-wide apps."
    }
  }]
}
